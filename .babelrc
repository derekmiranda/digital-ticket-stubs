{
  "presets": ["env", "react"],
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "test": {
      "plugins": [
        "transform-runtime",
        ["module-resolver", {
          "root": "./client",
          "alias": {
            "^(.+)": "./\\1"
          }
        }]
      ]
    }
  }
}
