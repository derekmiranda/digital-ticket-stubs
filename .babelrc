{
  "presets": ["env", "react"],
  "plugins": [
    "transform-object-rest-spread",
    ["module-resolver", {
      "alias": {
        "models": "./models",
        "utils": "./utils",
        "server": "./server",
        "^@(.+)": "./\\1"
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": ["transform-runtime"]
    }
  }
}
